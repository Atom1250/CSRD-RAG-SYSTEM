{
  "name": "csrd-rag-frontend-simple",
  "version": "1.0.0",
  "description": "Lightweight frontend for CSRD RAG System",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,js",
    "format": "prettier --write src/**/*.{ts,js,css,html}"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "csrd",
    "sustainability",
    "rag",
    "typescript",
    "vite"
  ],
  "author": "CSRD RAG System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/csrd-rag-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/csrd-rag-system/issues"
  },
  "homepage": "https://github.com/yourusername/csrd-rag-system#readme"
}